CREATE OR REPLACE TABLE silver.employee_performance AS
SELECT
  CAST(Employee_ID AS INT) AS employee_id,
  Department AS department,
  Gender AS gender,
  CAST(Age AS INT) AS age,
  Job_Title AS job_title,
  CAST(Hire_Date AS DATE) AS hire_date,
  CAST(Years_At_Company AS INT) AS years_at_company,
  Education_Level AS education_level,
  CAST(Performance_Score AS INT) AS performance_score,
  CAST(Monthly_Salary AS DOUBLE) AS monthly_salary,
  CAST(Work_Hours_Per_Week AS INT) AS work_hours_per_week,
  CAST(Projects_Handled AS INT) AS projects_handled,
  CAST(Overtime_Hours AS INT) AS overtime_hours,
  CAST(Sick_Days AS INT) AS sick_days,
  Remote_Work_Frequency AS remote_work_frequency,
  CAST(Team_Size AS INT) AS team_size,
  CAST(Training_Hours AS INT) AS training_hours,
  CAST(Promotions AS INT) AS promotions,
  CAST(Employee_Satisfaction_Score AS DOUBLE) AS employee_satisfaction_score,
  CAST(Resigned AS BOOLEAN) AS resigned
FROM
  bronze.employee_performance_raw
WHERE
  Employee_ID IS NOT NULL
